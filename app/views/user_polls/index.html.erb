<p id="notice"><%= notice %></p>

<h1>Listing User Polls</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Description</th>
      <th colspan="2"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_polls.each do |user_poll| %>
      <tr>
        <td><%= user_poll.title %></td>
        <td><%= user_poll.description %></td>
        <td><%= link_to 'Show', user_poll %></td>
        <td><%= link_to 'Destroy', user_poll, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User poll', new_user_poll_path %>
