<canvas data-poll-id=<%= raw("\"#{@user_poll.id}\"") %>></canvas>
<% @user_poll.poll_questions.each_with_index do |poll_question, index| %>
  <p>Question <%= "#{index+1}" %>. <%= poll_question.text %></p>
  <%= image_tag poll_question.poll_question_picture.url(:thumb) %><br /><br />
  <canvas data-question-id=<%= raw("\"#{poll_question.id}\"") %>></canvas> <br>
<% end %>
